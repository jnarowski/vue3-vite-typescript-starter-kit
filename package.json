{
  "name": "spectora-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:fix": "prettier . -w -u",
    "preview": "vite preview",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn test --silent",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "@formkit/vue": "^1.0.0-beta.6",
    "pinia": "^2.0.12",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@formkit/tailwindcss": "^1.0.0-beta.6",
    "@types/jest": "^27.4.1",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-standard": "^6.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "standard": "^16.0.4",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2",
    "vite": "^2.8.0",
    "vite-plugin-fonts": "^0.3.0",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  }
}
